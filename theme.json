						":hover": {
							"color": {
								"text": "var(--wp--preset--color--success)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/site-tagline": {
				"spacing": {
					"margin": {
						"bottom": "0",
						"top": "0"
					}
				}
			},
