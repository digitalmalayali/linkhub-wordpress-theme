					"outline": {
						"border": {
							"color": "var(--wp--preset--color--grey)",
							"radius": "4px",
							"style": "solid",
							"width": "1px"
						}
					}
				}
			},
			"core/details": {
				"color": {
					"text": "var(--wp--preset--color--error)"
				},
				"spacing": {
					"margin": {
						"bottom": "0.5em",
						"top": "0.4em"
					}
				}
			},
			"core/group": {
				"css": "margin-left: .2em;\nmargin-right: .2em;"
			},
			"core/image": {
				"css": ":hover {\n    opacity: .8;\n}"
			},
			"core/paragraph": {
				"elements": {
